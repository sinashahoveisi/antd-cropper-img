{
  "name": "@antd-cropper-img/root",
  "version": "1.1.0",
  "private": true,
  "description": "The best DrillDown package for react",
  "keywords": [
    "react",
    "antd",
    "ant-design",
    "upload",
    "image",
    "crop",
    "cropper"
